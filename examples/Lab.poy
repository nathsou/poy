extend Num {
    fun isPrime() {
        if self == 2 { return true }
        if self < 2 or self mod 2 == 0 { return false }
        mut i = 3

        while i * i <= self {
            if self mod i == 0 {
                return false
            }

            i += 2
        }

        true
    }

    fun sqrt() { Math.sqrt(self) }
    fun floor() { Math.floor(self) }
    fun ceil() { Math.ceil(self) }
    fun round() { Math.round(self) }
}

