import Foundations { print, typeOf, console, Console, ElementType }

struct IteratorResult<res> {
    value: res,
    done: Bool,
}

extend<a> a {
    fun print() {
        console.log(self)
        self
    }
}

print([1, 2, 3].zip(["a", "b", "c"]).reversed())
print("Num[]::reversed: " ++ typeOf(Num[]::reversed).show())
let ir = IteratorResult { value: "hello", done: false }
print("ir: " ++ typeOf(ir).show())
ir.print()
[1, 2, 3].print().reversed().print()
