
pub declare type {
    Unit
    Bool
    Str
    Num
    Nil
    Cons<head, tail>
    Tuple<elems>
    Array<t>
    Set<t>
    Function<args, ret>
    Iterator<elem>
    Map<key, val>
}

pub extend () {
    fun show(): Str {
        "()"
    }
}

pub extend Num {
   #as("toString")
   declare fun show(): Str
}

pub extend Bool {
    fun show(): Str {
        if self { "true" } else { "false" }
    }
}

pub extend Function<_, _> {
    fun show(): Str {
        "<fun>"
    }
}
